{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "lib": ["es2016", "dom"],
    "outDir": "dist",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "experimentalDecorators": true,
    "removeComments": true,
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "types": ["@figma/plugin-typings", "node", "@kkratterf/token-types"]
  }
}
